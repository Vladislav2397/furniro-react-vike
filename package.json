{
    "name": "@effector/vike-react-template",
    "private": true,
    "version": "23.0.0",
    "description": "Template for quickly starting a SSR Node.js application with vite, react, effector, and fastify",
    "packageManager": "pnpm@9.7.1+sha256.46f1bbc8f8020aa9869568c387198f1a813f21fb44c82f400e7d1dbde6c70b40",
    "scripts": {
        "env:dev": "dotenvx run -f .env.development.local -f .env.development --",
        "dev": "npm run env:dev -- node --no-warnings --loader ts-node/esm server/index.ts",
        "start": "cross-env NODE_ENV=production pnpm run server",
        "server": "node ./dist/cli/index",
        "build": "vite build && tsc -p server",
        "lint": "eslint . --max-warnings 0",
        "format": "prettier --write '*'",
        "e2e:start": "pnpm exec playwright test",
        "e2e": "pnpm run e2e:start"
    },
    "type": "module",
    "keywords": [
        "react",
        "typescript",
        "vite",
        "vike",
        "effector",
        "fastify",
        "starter",
        "boilerplate",
        "template"
    ],
    "author": {
        "name": "Vladislav GlaDiatoR",
        "url": "https://github.com/Vladislav2397"
    },
    "license": "MIT",
    "engines": {
        "node": ">=18.6.0"
    },
    "dependencies": {
        "@dotenvx/dotenvx": "^1.31.0",
        "@effector/reflect": "^9.2.0",
        "@farfetched/core": "^0.13.0",
        "@fastify/accepts": "^4.3.0",
        "@fastify/compress": "^7.0.3",
        "@fastify/cookie": "^9.3.1",
        "@fastify/cors": "^9.0.1",
        "@fastify/early-hints": "^1.0.1",
        "@fastify/helmet": "^11.1.1",
        "@fastify/rate-limit": "^9.1.0",
        "@fastify/static": "^7.0.4",
        "@withease/contracts": "^1.1.0",
        "clsx": "^2.1.1",
        "compression": "^1.7.4",
        "dotenv": "^16.4.5",
        "effector": "^23.2.2",
        "effector-factorio": "^1.3.0",
        "effector-react": "^23.2.1",
        "fastify": "^4.28.1",
        "patronum": "^2.2.0",
        "pino": "^9.3.2",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "sass": "^1.83.0",
        "vike": "0.4.184",
        "vike-react": "^0.5.2",
        "zod": "^3.23.8"
    },
    "devDependencies": {
        "@babel/parser": "^7.25.3",
        "@babel/plugin-syntax-import-attributes": "^7.24.7",
        "@playwright/test": "^1.49.1",
        "@trivago/prettier-plugin-sort-imports": "^4.3.0",
        "@types/compression": "^1.7.5",
        "@types/node": "^22.3.0",
        "@types/react": "^18.3.3",
        "@types/react-dom": "^18.3.0",
        "@vitejs/plugin-react": "^4.3.1",
        "cross-env": "^7.0.3",
        "pino-pretty": "^11.2.2",
        "prettier": "^3.3.3",
        "ts-node": "^10.9.2",
        "typescript": "^5.5.4",
        "vite": "^5.4.1",
        "vite-plugin-pretty-module-classnames": "^1.3.1",
        "vite-plugin-svgr": "^4.2.0"
    }
}
